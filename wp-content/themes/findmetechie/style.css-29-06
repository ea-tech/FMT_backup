.service-overlayer {
	position: relative;
	color: #fff;
}
.service-overlayer::before {
 background: rgba(0, 0, 0, 0.8);
 content: "";
 height: 100%;
 left: 0;
 opacity: 0.8;
 -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
 position: absolute;
 top: 0;
 width: 100%;
}
